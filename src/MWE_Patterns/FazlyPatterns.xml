                        <pat><w pos="NN2"     /></pat>
                        <pat><w pos="NN2-VVZ" /></pat>
                        <pat><w pos="VVZ-NN2" /></pat>
                    </either>
                </pat>
            </pat>

            <!-- Pattern 2 -->
            <pat>
                <!--Active Verb-->
                <w pos="V*" />

                <!--Ignorable Tokens-->
                <pat ignore="true">
