                            <w pos="PUR" />
                        </pat>
                        <pat repeat="*"><w pos="PUN"><neg lemma="." /><neg lemma="!" /><neg lemma="?" /><neg lemma=":" /><neg lemma=";" /></w></pat>
                        <pat repeat="*"><w pos="T*" /></pat>
                        <pat repeat="*"><w pos="U*" /></pat>
                        <pat repeat="*"><w pos="X*" /></pat>
                        <pat repeat="*"><w pos="Z*" /></pat>
                    </pat>
                </pat>

                <!--det:a/an-->
                <pat>
                    <either>
                        <pat><w lemma="a" pos="AT0"/></pat>
                        <pat><w lemma="an" pos="AT0"/></pat>
                    </either>
                </pat>

                <!--Ignorable Tokens-->
                <pat ignore="true">
                    <pat repeat="*">
                        <pat repeat="*"><w pos="A*" /></pat>
                        <pat repeat="*"><w pos="C*" /></pat>
                        <pat repeat="*"><w pos="N*" /></pat>
